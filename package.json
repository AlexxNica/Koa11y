{
    "name": "koa11y",
    "version": "2.0.0",
    "author": "The Jared Wilcurt",
    "description": "Check if a website has accessibility issues.",
    "keywords": [ "Accessibility", "standards", "automated", "WCAG", "Section 508" ],
    "maintainers": [{
        "name": "TheJaredWilcurt",
        "email": "TheJaredWilcurt@noreply.github.com",
        "web": "http://github.com/TheJaredWilcurt"
    }],
    "repository": [{
        "type": "git",
        "url": "git://github.com/TheJaredWilcurt/Koa11y.git"
    }],
    "bugs": "https://github.com/TheJaredWilcurt/Koa11y/issues",
    "main": "index.htm",
    "window": {
        "title": "Koa11y",
        "icon": "_img/icon32.png",
        "toolbar": false,
        "resizable": true,
        "visible": true,
        "transparent": false,
        "width": 900,
        "height": 550,
        "min_width": 400,
        "min_height": 200,
        "max_width": 8000,
        "max_height": 8000,
        "position": "center",
        "always-on-top": false,
        "show_in_taskbar": true,
        "fullscreen": false,
        "frame": true,
        "as_desktop": false
    },
    "nodejs": true,
    "single-instance": true,
    "node-main": "_scripts/node.js",
    "webkit": {
        "page-cache": false,
        "java": false,
        "plugin": true
    },
    "dependencies": {
        "pa11y": "4.1.1",
        "phantomjs-prebuilt": "^2.1.14",
        "json2csv": "3.7.3",
        "base64-img": "^1.0.3",
        "fs-extra": "^0.30.x",
        "semver": "^5.3.x"
    },
    "devDependencies": {
        "nw": "^0.12.3",
        "eslint": "^3.8.1",
        "sass-lint": "^1.10.2"
    },
    "scripts": {
        "start": "nw .",
        "phantomjs": "phantomjs",
        "phantomjs-prebuilt": "phantomjs-prebuilt",
        "lint": "eslint --config=.eslintrc.js _scripts sasslinter.js",
        "fix": "eslint --fix --config=.eslintrc.js _scripts sasslinter.js",
        "sasslint": "node sasslinter.js"
    },
    "license": "MIT",
    "licenses": [{
        "type": "MIT",
        "url": "https://github.com/TheJaredWilcurt/Koa11y/blob/master/LICENSE"
    }]
}
